<services>
    <!-- list pypi modules -->
    <pypi>
        <module>a2p2</module>
    </pypi>
    <!-- list repo that represent an application or public library without associated instance -->
    <repos>
        <github>
            <location>https://github.com/JMMC-OpenDev/oitools/</location>
            <category>Java</category>
        </github>
    </repos>
    <!-- list public existdb applications -->
    <exist>
        <app>
            <name>A2P2W</name>
            <release>
                <status>public</status>
                <location>https://a2p2w.jmmc.fr/</location>
            </release>
            <release>
                <status>beta</status>
                <location>https://a2p2w-beta.jmmc.fr/</location>
            </release>
        </app>
        <app>
            <name>CatalogAPI</name>
            <release>
                <status>public</status>
                <location>https://oidb.jmmc.fr/exist/apps/catalogs/</location>
            </release>
            <release>
                <status>beta</status>
                <location>https://oidb-beta.jmmc.fr/exist/apps/catalogs/</location>
            </release>
        </app>
        <!-- app>
            <name>DataModels</name>
            <release>
                <status>public</status>
                <location>https://exist.jmmc.fr/datamodels/</location>
            </release>
          </app -->
        <app>
            <name>SearchFTT</name>
            <release>
                <status>public</status>
                <location>https://searchftt.jmmc.fr/</location>
            </release>
            <release>
                <status>beta</status>
                <location>https://searchftt-beta.jmmc.fr/</location>
            </release>
        </app>
       <app>
            <name>Releases</name>
            <release>
                <status>public</status>
                <location>https://releases.jmmc.fr/</location>
            </release>
            <release>
                <status>beta</status>
                <location>https://releases-beta.jmmc.fr/</location>
            </release>
        </app>
        <app>
            <name>VOAR</name>
            <icon-url>https://voar.jmmc.fr/resources/images/voar49x64.png</icon-url>
            <release>
                <status>public</status>
                <location>https://voar.jmmc.fr/</location>
            </release>
            <release>
                <status>beta</status>
                <location>https://voar-beta.jmmc.fr/</location>
            </release>
        </app>
        <app>
            <name>OiVal</name>
            <release>
                <status>public</status>
                <location>https://oival.jmmc.fr/</location>
            </release>
            <release>
                <status>beta</status>
                <location>https://oival-beta.jmmc.fr/</location>
            </release>
        </app>
        <app>
            <name>OiDB</name>
            <icon-url>https://oidb.jmmc.fr/resources/images/oidb-7-tr.png</icon-url>
            <release>
                <status>public</status>
                <location>https://oidb.jmmc.fr/</location>
            </release>
            <release>
                <status>beta</status>
                <location>https://oidb-beta.jmmc.fr/</location>
            </release>
        </app>
    </exist>
    <!-- list simple web applications -->
    <web>
        <app>
            <name>AMHRA</name>
            <icon-url>https://amhra.oca.eu/AMHRA/pictures/logoamhra-50-a.png</icon-url>
            <release>
                <status>public</status>
                <location>https://amhra.oca.eu/</location>
            </release>
        </app>
        <app>
            <name>BadCal</name>
            <release>
                <status>public</status>
                <location>https://jmmc.fr/badcal/</location>
            </release>
        </app>
        <app>
            <name>GetStar</name>
            <release>
                <status>public</status>
                <location>https://www.jmmc.fr/english/tools/proposal-preparation/getstar-61/</location>
            </release>
            <release>
                <status>beta</status>
                <location>https://apps.jmmc.fr/~betaswmgr/getstar/</location>
            </release>
        </app>
        <app>
            <name>ObsPortal</name>
            <release>
                <status>public</status>
                <location>https://obs.jmmc.fr/</location>
            </release>
            <release>
                <status>beta</status>
                <location>https://obs-preprod.jmmc.fr/</location>
            </release>
        </app>
    </web>
    <server>
        <url>https://www.jmmc.fr</url>
        <jnlp>
            <name>AppLauncher</name>
            <release>
                <status>public</status>
                <location>/apps/public/AppLauncher/</location>
                <comment>public</comment>
            </release>
            <release>
                <status>beta</status>
                <location>/apps/beta/AppLauncher/</location>
                <comment>beta</comment>
            </release>
            <release>
                <status>dev</status>
                <location>/apps/alpha/AppLauncher/</location>
                <comment>dev</comment>
            </release>
        </jnlp>
        <jnlp>
            <name>Aspro2</name>
            <release>
                <status>public</status>
                <location>/apps/public/Aspro2/</location>
                <comment>public</comment>
            </release>
            <release>
                <status>beta</status>
                <location>/apps/beta/Aspro2/</location>
                <comment>beta</comment>
            </release>
            <release>
                <status>dev</status>
                <location>/apps/alpha/Aspro2/</location>
                <comment>dev</comment>
            </release>
        </jnlp>
        <jnlp>
            <name>LITpro</name>
            <release>
                <status>public</status>
                <location>/apps/public/LITpro/</location>
                <comment>public</comment>
            </release>
            <release>
                <status>beta</status>
                <location>/apps/beta/LITpro/</location>
                <comment>beta</comment>
            </release>
            <release>
                <status>dev</status>
                <location>/apps/alpha/LITpro/</location>
                <comment>dev</comment>
            </release>
        </jnlp>
        <jnlp>
            <name>OIFitsExplorer</name>
            <release>
                <status>public</status>
                <location>/apps/public/OIFitsExplorer/</location>
                <comment>public</comment>
            </release>
            <release>
                <status>beta</status>
                <location>/apps/beta/OIFitsExplorer/</location>
                <comment>beta</comment>
            </release>
            <release>
                <status>dev</status>
                <location>/apps/alpha/OIFitsExplorer/</location>
                <comment>dev</comment>
            </release>
        </jnlp>
        <jnlp>
            <name>OImaging</name>
            <release>
                <status>public</status>
                <location>/apps/public/OImaging/</location>
                <comment>public</comment>
            </release>
            <release>
                <status>beta</status>
                <location>/apps/beta/OImaging/</location>
                <comment>beta</comment>
            </release>
            <release>
                <status>dev</status>
                <location>/apps/alpha/OImaging/</location>
                <comment>dev</comment>
            </release>
        </jnlp>
        <jnlp>
            <name>SearchCal</name>
            <release>
                <status>public</status>
                <oldlocation>/~sclws/SearchCal/</oldlocation>
                <location>/apps/public/SearchCal/</location>
                <comment>public ([[https://www.jmmc.fr/~sclws/sclws_proxy.php][server proxy]] relaying tcp/8079 server)</comment>
            </release>
            <release>
                <status>beta</status>
                <location>/apps/beta/SearchCal/</location>
                <comment>beta ([[https://www.jmmc.fr/apps/beta/sclws_proxy.php][server proxy]] relaying tcp/7079 server)</comment>
            </release>
            <release>
                <status>dev</status>
                <location>/apps/alpha/SearchCal/</location>
                <comment>Development only ([[https://www.jmmc.fr/apps/beta/sclws_proxy.php][server proxy]] relaying secret port server)</comment>
            </release>
        </jnlp>
    </server>
</services>
